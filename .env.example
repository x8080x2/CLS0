# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
ADMIN_ID=your_telegram_admin_user_id

# Server Configuration
NODE_ENV=development
PORT=8000
LOG_LEVEL=info

# Webhook Configuration (for production)
WEBHOOK_DOMAIN=https://yourdomain.com

# WHM/cPanel API Configuration
WHM_SERVER=https://your-whm-server.com:2087
WHM_USERNAME=your_whm_username
WHM_PASSWORD=your_whm_password
WHM_PACKAGE_NAME=default

# Default redirect URL for script files
DEFAULT_REDIRECT_URL=https://your-redirect-destination.com

# Optional: Additional configuration
MAX_DOMAINS_PER_USER=5
RATE_LIMIT_MINUTES=60
